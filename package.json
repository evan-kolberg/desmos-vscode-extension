{
  "name": "desmos-calculator",
  "displayName": "Desmos",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "desmosViewContainer",
          "title": "Desmos",
          "icon": "./desmos.png"
        }
      ]
    },
    "views": {
      "desmosViewContainer": [
        {
          "id": "desmosCalcView",
          "name": "Desmos Calculator",
          "icon": "./desmos.png"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.openDesmosOffline",
        "title": "Desmos: Open Desmos Offline (v1.10.1)"
      },
      {
        "command": "extension.openDesmosOnline",
        "title": "Desmos: Open Desmos via API (v1.10.1)"
      },
      {
        "command": "extension.exportJson",
        "title": "Desmos: Export Work (.json)"
      },
      {
        "command": "extension.importJson",
        "title": "Desmos: Import Work (.json)"
      },
      {
        "command": "extension.clearUnsavedWork",
        "title": "Desmos: Clear All Unsaved Recovery Items"
      }
    ]
  },
  "dependencies": {
    "vscode": "^1.1.34"
  }
}
