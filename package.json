{
  "name": "desmos-calculator",
  "displayName": "Desmos",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "./extension.js",
  "icon": "./desmos.png",
  "activationEvents": [
    "onCommand:extension.openDesmosOffline",
    "onCommand:extension.openDesmosOnline",
    "onCommand:extension.exportJson",
    "onCommand:extension.importJson",
    "onCommand:extension.clearUnsavedWork",
    "onCommand:extension.recoverWork"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/evan-kolberg/desmos-vscode-extension"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "desmosViewContainer",
          "title": "Desmos",
          "icon": "./desmos.png"
        }
      ]
    },
    "views": {
      "desmosViewContainer": [
        {
          "id": "desmosCalcView",
          "name": "Desmos Calculator",
          "icon": "./desmos.png"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.openDesmosOffline",
        "title": "Desmos: Open Desmos Offline (v1.10.1)"
      },
      {
        "command": "extension.openDesmosOnline",
        "title": "Desmos: Open Desmos via API (v1.10.1)"
      },
      {
        "command": "extension.exportJson",
        "title": "Desmos: Export Work (.json)"
      },
      {
        "command": "extension.importJson",
        "title": "Desmos: Import Work (.json)"
      },
      {
        "command": "extension.clearUnsavedWork",
        "title": "Desmos: Clear Recovery Items"
      }
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "vscode": "^1.1.34"
  }
}
